<%= t('.header', title: @answer.question.body) %>
<br><br>
<%= form_with model: [:admin, @question, @answer], method: verb do |form| %>
  <%= form.label :text %>
  <%= form.text_field :body, id: :answer_text %>
  <%= form.label :correct %>
  <%= form.check_box :correct, id: :answer_correct %>
  <%= form.submit %>
<% end %>