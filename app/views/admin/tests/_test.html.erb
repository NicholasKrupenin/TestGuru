<tbody>
  <tr>
    <td><%= link_to test.title, admin_test_path(test), method: :get %></td>
    <td><%= link_to t(:update, scope: :base), edit_admin_test_path(test),
                                                           class: 'btn btn-outline-primary' %>
      <%= link_to t(:delete, scope: :base), admin_test_path(test), method: :delete,
        class: 'btn btn-outline-primary', data: { confirm: t(:delete_confirm, scope: :base) } %>

      <%= button_to t('.start'), start_test_path(test), class: 'btn btn-outline-primary' %></td>
    <td><%= test.questions.count %></td>
  </tr>
</tbody>
